
	<!-- HOME -->
	<section id="home" class="module-hero module-parallax module-fade bg-dark-30" data-background=  <%= asset_path "7subway1.jpg"%>>

		<div class="hs-caption container">
			<div class="caption-content">
				<div class="hs-title-size-4 font-alt m-b-30">
					Web Developer
				</div>
				<div class="hs-title-size-1 font-inc">
					Graduate of Flatiron School 2016
				</div>
			</div>
		</div>

	</section >
	<!-- /HOME -->

	<!-- WRAPPER -->
	<div class="wrapper">

		  <!-- PORTFOLIO -->
		  <section class="module-small p-t-20 p-b-0 p-t-sm-0">

		    <div class="container hidden-xs">

		      <!-- PORTFOLIO FILTER -->
		      <div class="row">
		        <div class="col-sm-12">

		          <ul id="filters" class="filter font-inc">
		            <li><a href="#" data-filter="*" class="current wow fadeInUp">All</a></li>
								<li><a href="#" data-filter=".rubyonrailsapi" class="wow fadeInUp" data-wow-delay="0.2s">Ruby on Rails API</a></li>
								<li><a href="#" data-filter=".javascript" class="wow fadeInUp" data-wow-delay="0.2s">JavaScript</a></li>
								<li><a href="#" data-filter=".nodejs" class="wow fadeInUp" data-wow-delay="0.2s">Node.js</a></li>
								<li><a href="#" data-filter=".postgresql" class="wow fadeInUp" data-wow-delay="0.2s">PostgreSQL</a></li>
								<li><a href="#" data-filter=".redis" class="wow fadeInUp" data-wow-delay="0.2s">Redis</a></li>

							</ul>

		        </div>
		      </div>
		      <!-- /PORTFOLIO FILTER -->

		    </div>

		    <!-- PORTFOLIO LIST -->
		    <ul id="works-grid" class="works-grid works-grid-masonry works-grid-3 works-hover-w">

					<% @projects.each do |project| %>
		      <!-- PORTFOLIO ITEM -->
		      <li class="work-item <%=project.tags%> col-md-3 col-lg-3" style="padding-bottom:30px;">
		        <%= link_to project_path(project) do %>
		          <div class="work-image">
		              <%= image_tag project.image_upload.url(:medium) %>

		          </div>
		          <div class="work-caption">
		            <h3 class="work-title font-alt"><%= project.name %></h3>
		            <div class="work-descr font-inc">
		              <%=project.description  %>
		            </div>
		          </div>
		        <% end %>
		      </li>
		      <!-- /PORTFOLIO ITEM -->
					<% end %>

		    </ul>
		    <!-- /PORTFOLIO LIST -->

		  </section>
		  <!-- /PORTFOLIO -->

			<!-- TESTIMONIAL -->
			<section class="module-small">

				<div class="container">

					<div class="row">

						<!-- MODULE TITLE -->
						<div class="col-sm-6">
							<h2 class="module-title font-alt">"Be bold and courageous...</h2>
						</div>
						<!-- /MODULE TITLE -->

						<!-- BLOCKQUOTE -->
						<div class="col-sm-6">
							<blockquote class="font-serif align-right">
								<p>When you look back on your life, you'll regret the things you didn't do more than the ones you did."</p>
								<p class="font-inc font-uppercase">-H. Jackson Brown, Jr.</p>
							</blockquote>
						</div>
						<!-- /BLOCKQUOTE -->

					</div>

				</div>

			</section>
			<!-- /TESTIMONIAL -->


	</div>
	<!-- /WRAPPER -->
